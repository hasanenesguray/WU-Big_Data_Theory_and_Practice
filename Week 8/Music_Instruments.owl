<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/owl/musicInstruments#"
     xml:base="http://www.example.org/owl/musicInstruments"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.example.org/owl/musicInstruments">
        <rdfs:comment>An ontology representing various musical instruments and their associated genres.</rdfs:comment>
    </owl:Ontology>
    
    <!-- Classes -->
    <owl:Class rdf:about="http://www.example.org/owl/musicInstruments#MusicalInstrument">
        <rdfs:label>Musical Instrument</rdfs:label>
        <rdfs:comment>A general class for all types of musical instruments.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="http://www.example.org/owl/musicInstruments#StringInstrument">
        <rdfs:subClassOf rdf:resource="http://www.example.org/owl/musicInstruments#MusicalInstrument"/>
        <rdfs:label>String Instrument</rdfs:label>
        <rdfs:comment>A class representing string-based musical instruments.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="http://www.example.org/owl/musicInstruments#WindInstrument">
        <rdfs:subClassOf rdf:resource="http://www.example.org/owl/musicInstruments#MusicalInstrument"/>
        <rdfs:label>Wind Instrument</rdfs:label>
        <rdfs:comment>A class for wind-based musical instruments.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="http://www.example.org/owl/musicInstruments#PercussionInstrument">
        <rdfs:subClassOf rdf:resource="http://www.example.org/owl/musicInstruments#MusicalInstrument"/>
        <rdfs:label>Percussion Instrument</rdfs:label>
        <rdfs:comment>A class covering percussion-based musical instruments.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="http://www.example.org/owl/musicInstruments#ElectronicInstrument">
        <rdfs:subClassOf rdf:resource="http://www.example.org/owl/musicInstruments#MusicalInstrument"/>
        <rdfs:label>Electronic Instrument</rdfs:label>
        <rdfs:comment>A class representing electronic musical instruments.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="http://www.example.org/owl/musicInstruments#Genre">
        <rdfs:label>Genre</rdfs:label>
        <rdfs:comment>A class representing musical genres.</rdfs:comment>
    </owl:Class>

    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#SteinwayGrandPiano">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#StringInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Steinway &amp; Sons</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#ClassicalMusic"/>
        <rdfs:label>Steinway Grand Piano</rdfs:label>
        <rdfs:comment>A grand piano manufactured by Steinway &amp; Sons, commonly used in classical music.</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- Additional genres -->
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#Jazz">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#Genre"/>
        <rdfs:label>Jazz</rdfs:label>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#Rock">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#Genre"/>
        <rdfs:label>Rock</rdfs:label>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#ElectronicMusic">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#Genre"/>
        <rdfs:label>Electronic Music</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Additional musical instruments -->
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#FenderStratocaster">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#StringInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fender</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#Rock"/>
        <rdfs:label>Fender Stratocaster</rdfs:label>
        <rdfs:comment>An iconic electric guitar commonly used in rock music.</rdfs:comment>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#YamahaSaxophone">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#WindInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Yamaha</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#Jazz"/>
        <rdfs:label>Yamaha Saxophone</rdfs:label>
        <rdfs:comment>A popular saxophone model frequently used in jazz music.</rdfs:comment>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#RolandTR808">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#ElectronicInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Roland</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#ElectronicMusic"/>
        <rdfs:label>Roland TR-808</rdfs:label>
        <rdfs:comment>An influential drum machine widely used in electronic music.</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- Additional musical instruments -->
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#GibsonLesPaul">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#StringInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gibson</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#Rock"/>
        <rdfs:label>Gibson Les Paul</rdfs:label>
        <rdfs:comment>An iconic electric guitar often associated with rock and blues music.</rdfs:comment>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#FenderPrecisionBass">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#StringInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fender</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#Rock"/>
        <rdfs:label>Fender Precision Bass</rdfs:label>
        <rdfs:comment>A classic bass guitar known for its deep sound, commonly used in rock and funk music.</rdfs:comment>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#Saxophone">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#WindInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Various</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#Jazz"/>
        <rdfs:label>Saxophone</rdfs:label>
        <rdfs:comment>A versatile wind instrument used in various musical genres, prominently in jazz.</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Additional percussion instrument -->
    <owl:NamedIndividual rdf:about="http://www.example.org/owl/musicInstruments#DrumKit">
        <rdf:type rdf:resource="http://www.example.org/owl/musicInstruments#PercussionInstrument"/>
        <hasManufacturer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Various</hasManufacturer>
        <isCommonInGenre rdf:resource="http://www.example.org/owl/musicInstruments#Rock"/>
        <rdfs:label>Drum Kit</rdfs:label>
        <rdfs:comment>A set of percussion instruments commonly used in rock music.</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>
