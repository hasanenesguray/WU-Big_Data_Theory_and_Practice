<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/ontology#"
     xml:base="http://example.org/ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
    
    <owl:Ontology rdf:about="http://example.org/ontology"/>
    
    <!-- Classes -->
    <owl:Class rdf:about="#CelestialBody">
        <rdfs:label>Celestial Body</rdfs:label>
        <rdfs:comment>A general class for any celestial object.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Galaxy">
        <rdfs:subClassOf rdf:resource="#CelestialBody"/>
        <rdfs:label>Galaxy</rdfs:label>
        <rdfs:comment>A massive, gravitationally bound system of stars, stellar remnants, interstellar gas, dust, and dark matter.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Star">
        <rdfs:subClassOf rdf:resource="#CelestialBody"/>
        <rdfs:label>Star</rdfs:label>
        <rdfs:comment>A luminous sphere of plasma held together by its own gravity.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Planet">
        <rdfs:subClassOf rdf:resource="#CelestialBody"/>
        <rdfs:label>Planet</rdfs:label>
        <rdfs:comment>A celestial body orbiting a star or stellar remnant that is massive enough to be rounded by its own gravity.</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="#Moon">
        <rdfs:subClassOf rdf:resource="#CelestialBody"/>
        <rdfs:label>Moon</rdfs:label>
        <rdfs:comment>A natural satellite orbiting a planet.</rdfs:comment>
    </owl:Class>
  
   <!-- Properties -->
    <owl:DatatypeProperty rdf:about="#hasName">
        <rdfs:domain rdf:resource="#CelestialBody"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasType">
        <rdfs:domain rdf:resource="#CelestialBody"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasTemperature">
        <rdfs:domain rdf:resource="#Star"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasMass">
        <rdfs:domain rdf:resource="#Planet"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="#MilkyWay">
        <rdf:type rdf:resource="#Galaxy"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Milky Way</hasName>
        <hasType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Barred Spiral</hasType>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Sun">
        <rdf:type rdf:resource="#Star"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sun</hasName>
        <hasType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">G-Type Main-Sequence</hasType>
        <hasTemperature rdf:datatype="http://www.w3.org/2001/XMLSchema#float">5778</hasTemperature>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Earth">
        <rdf:type rdf:resource="#Planet"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Earth</hasName>
        <hasMass rdf:datatype="http://www.w3.org/2001/XMLSchema#float">5.972</hasMass>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Mars">
        <rdf:type rdf:resource="#Planet"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mars</hasName>
        <hasMass rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.641</hasMass>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Moon">
        <rdf:type rdf:resource="#Moon"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Moon</hasName>
        <hasMass rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.073</hasMass>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Phobos">
        <rdf:type rdf:resource="#Moon"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Phobos</hasName>
        <hasMass rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.0659e-8</hasMass>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="#Deimos">
        <rdf:type rdf:resource="#Moon"/>
        <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Deimos</hasName>
        <hasMass rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.4762e-8</hasMass>
    </owl:NamedIndividual>
    
</rdf:RDF>
